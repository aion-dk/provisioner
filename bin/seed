#!/bin/sh

set -e

echo "setting up database..."
./db-init.sh &> /dev/null

echo "setting a current election..."
curl -XPOST 'http://provisioner:3009/setCurrentElection' \
  -H 'Content-Type: application/json' \
  -d '{"electionId": "0c7a901a-e8fe-4601-8417-5d0823159d42"}' \
  > /dev/null 2>&1

echo "done"
